+/* SRAM */
+#define CONFIG_SYS_TEXT_BASE		0x200a0000
+#define CONFIG_SYS_STAY_IN_SRAM
+/* Very early stack, 320KB above start of U-Boot image */
+#define CONFIG_SYS_INIT_SP_ADDR		(CONFIG_SYS_TEXT_BASE + (320 * 1024) - 4)
