+#define	USBH_POTPGT_WAIT_TIME			(0xFF << 24)
+#define	USBH_NOCP_ENABLE			0
+#define	USBH_OCPM_PORT_UNIT			BIT(11)
+#define	USBH_NPS_ALWAYS_POWERON			BIT(9)
+#define	USBH_PSM_PORT_UNIT			BIT(8)
