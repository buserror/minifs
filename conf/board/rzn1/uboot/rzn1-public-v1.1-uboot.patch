+
+#define CONFIG_SYS_THUMB_BUILD
+#define CONFIG_SYS_LONGHELP
+#define	CONFIG_CMDLINE_EDITING
+#define CONFIG_VERSION_VARIABLE		/* include version env variable */
+/* debug purpose */
+#define CONFIG_MD5
+#define CONFIG_CMD_MD5SUM
+
