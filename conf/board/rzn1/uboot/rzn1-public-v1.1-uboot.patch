+#define CONFIG_SYS_MEMTEST_SCRATCH	RZN1_SRAM_SYS_BASE
+#undef CONFIG_SYS_SDRAM_SIZE
+#define CONFIG_SYS_SDRAM_SIZE		(DDR_MAX_SIZE * 1024 * 1024)
